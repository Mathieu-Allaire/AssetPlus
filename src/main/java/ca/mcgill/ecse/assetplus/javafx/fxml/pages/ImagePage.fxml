      <Button fx:id="addDriverButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#addDriverClicked" prefHeight="40.0" text="Add Image" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2" />
      <Button fx:id="addRouteButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#addRouteClicked" prefHeight="40.0" text="Delete Image" GridPane.columnIndex="3" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2" />
      <Label alignment="CENTER" prefWidth="400.0" text="ADD IMAGE TO TICKET" textFill="WHITE" GridPane.columnSpan="2" GridPane.hgrow="ALWAYS">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="15.0" layoutY="21.0" prefWidth="400.0" text="DELETE IMAGE FROM TICKET" GridPane.columnIndex="2" GridPane.columnSpan="2">
