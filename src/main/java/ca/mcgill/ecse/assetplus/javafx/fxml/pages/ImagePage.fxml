<GridPane hgap="10.0" prefHeight="115.0" prefWidth="726.0" vgap="5.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ca.mcgill.ecse.assetplus.javafx.fxml.controllers.ImagePageController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="178.0" minWidth="0.0" />
      <ColumnConstraints hgrow="ALWAYS" maxWidth="561.0" minWidth="4.0" prefWidth="150.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="561.0" minWidth="0.0" prefWidth="105.0" />
      <ColumnConstraints hgrow="ALWAYS" maxWidth="561.0" minWidth="10.0" prefWidth="150.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints maxHeight="91.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="91.0" minHeight="10.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="101.0" minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <Label alignment="CENTER_RIGHT" prefHeight="18.0" prefWidth="106.0" text="Image URL:" GridPane.rowIndex="1" />
      <TextField fx:id="driverNameTextField" prefHeight="25.0" prefWidth="153.0" promptText="Enter image URL" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" />
      <Label alignment="CENTER_RIGHT" prefHeight="15.0" prefWidth="105.0" text="Image URL:" GridPane.columnIndex="2" GridPane.rowIndex="1" />
